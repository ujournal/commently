        "flarum/core": "^1.8",
        "flarum/emoji": "*",
        "flarum/flags": "*",
        "flarum/lang-english": "*",
        "flarum/likes": "*",
        "flarum/lock": "*",
        "flarum/markdown": "*",
        "flarum/mentions": "*",
        "flarum/nicknames": "*",
        "flarum/pusher": "*",
        "flarum/statistics": "*",
        "flarum/sticky": "*",
        "flarum/subscriptions": "*",
        "flarum/suspend": "*",
        "flarum/tags": "*",
        "fof/upload": "*",
        "league/flysystem-aws-s3-v3": "^1.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {"preferred-install": "dist", "sort-packages": true},
    "autoload": {
        "psr-4": {"App\\": "app/"}
