{
    "name": "flarum/flarum",
    "description": "Delightfully simple forum software.",
    "type": "project",
    "keywords": ["forum", "discussion"],
    "homepage": "https://flarum.org/",
    "license": "MIT",
    "authors": [{"name": "Flarum", "email": "info@flarum.org", "homepage": "https://flarum.org/team"}],
