{
    "name": "ujournal/commently",
    "description": "Commently is modification of Flarum forum software.",
    "type": "project",
    "keywords": ["forum", "discussion"],
    "homepage": "https://github.com/ujournal/commently",
    "license": "MIT",
    "authors": [{"name": "UJournal", "email": "info@ujournal.com", "homepage": "https://ujournal.com.ua"}],
    "support": {"issues": "https://github.com/ujournal/commently/issues", "source": "https://github.com/ujournal/commently", "docs": "https://github.com/ujournal/commently"},
    "repositories": [{"type": "path", "url": "extensions/upload-dimensions"}, {"type": "path", "url": "extensions/sort-by-likes"}],
    "require": {
        "ujournal/upload-dimensions": "^1.0",
        "ujournal/sort-by-likes": "^1.0",
        "blomstra/s3-assets": "*",
        "flarum/approval": "*",
        "flarum/bbcode": "*",
        "flarum/core": "^1.8",
        "flarum/emoji": "*",
        "flarum/flags": "*",
        "flarum/lang-english": "*",
        "flarum/likes": "*",
        "flarum/lock": "*",
        "flarum/markdown": "*",
        "flarum/mentions": "*",
        "flarum/nicknames": "*",
        "flarum/pusher": "*",
        "flarum/statistics": "*",
        "flarum/sticky": "*",
        "flarum/subscriptions": "*",
        "flarum/suspend": "*",
        "flarum/tags": "*",
        "fof/upload": "*",
        "league/flysystem-aws-s3-v3": "^1.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.0"
        }
    },
    "autoload": {
        "psr-4": {"App\\": "app/"}
    }
}
